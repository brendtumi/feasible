{
  "name": "feasible",
  "version": "4.0.0",
  "description": "A question-answer based, configurable and automated development environment setup wizard that helps automate and standardize development environments across teams.",
  "main": "lib/index.js",
  "type": "module",
  "bin": {
    "feasible": "bin/cli.js"
  },
  "scripts": {
    "test": "mocha",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brendtumi/feasible.git"
  },
  "author": "Tümay Çeber <tumayceber@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brendtumi/feasible/issues"
  },
  "homepage": "https://github.com/brendtumi/feasible#readme",
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "chalk": "5.4.1",
    "commander": "13.1.0",
    "debug": "4.4.0",
    "exec-sh": "0.4.0",
    "globby": "14.1.0",
    "inquirer": "12.5.0",
    "isolated-vm": "5.0.4",
    "js-yaml": "4.1.0",
    "json5": "2.2.3",
    "lodash.get": "4.4.2",
    "make-dir": "5.0.0",
    "update-notifier": "7.3.1",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "chai": "5.2.0",
    "eslint": "8.57.1",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-promise": "6.6.0",
    "mocha": "11.1.0"
  },
  "eslintConfig": {
    "env": {
      "browser": false,
      "node": true,
      "es2022": true,
      "mocha": true,
      "shelljs": true
    },
    "extends": "standard",
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "semi": [
        2,
        "always"
      ]
    }
  }
}
