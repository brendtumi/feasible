{
  "repository": {
    "url": "https://mygithub.gsk.com/gsk-tech/commitizen-semver-nodejs-template.git",
    "target": "tmp1"
  },
  "variables": {
    "VAR1": {
      "question": "What is variable 1",
      "type": "input"
    },
    "VAR2": {
      "question": "What is variable 2",
      "type": "number",
      "initial": 42
    },
    "VAR3": {
      "question": "What is variable 3",
      "type": "list",
      "options": ["test 1", "test 2", "test 3"]
    },
    "VAR4": {
      "question": "What is variable 4",
      "type": "list",
      "options": [1, 2, 3, 4, 5, 6]
    },
    "VAR5": {
      "question": "What is variable 5",
      "type": "password"
    }
  },
  "defaults": {
    "DEF1": "default var 1",
    "DEF2": 2
  },
  "actions": {
    "pre": ["echo ${VAR1}", "echo ${VAR1.val}", "yarn"],
    "post": ["echo 'post'"]
  },
  "files": {
    "test/.env1": "${DEF1}\n${VAR1}\nTEST_VAR_3=${VAR3.val}\n${DEF1}\n${VAR1}\nTEST_VAR_3=${VAR4.val}\n\n${DEF1}\n${VAR1}\nTEST_VAR_3=${VAR3.val}",
    "test/.env2": "${DEF2}\n${VAR2}\n${VAR5}",
    "test/super.test.yml": {
      "type": "yaml",
      "variables": ["VAR1", "VAR2", ["NEW_NAME", "VAR3"], ["NEW_NAME2", "VAR3"], "DEF1"]
    },
    "test/super3.env": {
      "type": "env",
      "variables": ["VAR1", "VAR2", ["NEW_NAME", "VAR3"], ["NEW_NAME2", "VAR3"], "DEF1"]
    }
  }
}
